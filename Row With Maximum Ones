class Solution {
public:
    vector<int> rowAndMaximumOnes(vector<vector<int>>& mat) {
       vector<int>result(2,0);
       int m=mat.size();
       int n=mat[0].size();
       int count;
       for(int i=0;i<m;i++) {
        count=0;
        for(int j=0;j<n;j++) {
            count+=mat[i][j];
        }
        if(count>result[1]) {
            result[0]=i;
            result[1]=count;
        }
       } 
       return result;
    }
};
